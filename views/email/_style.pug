style(type="text/css").

  @media screen and (max-width: 640px) {
    
  *[class="navline"] {
    width: 100%;
    height: auto;
    border-top: 1px solid #e9e9e9;
    border-bottom: none;
  }
  
  .navContainer {
    position: relative;
  }
  
  *[class="navContainer"] td {
    margin: auto;
    display: block;
    width: 95%;
    border-bottom: 1px solid #e9e9e9;
  }
  
  .navContainer td:last-child {
    width: 100%;
  }
  
  .navContainer td a {
    display: block;
    height: 40px;
  }
  
  #menuContainer {
    position: relative;
    overflow: hidden;
  }
  
  .navContainer table {
    margin-top: -500px;
    -ms-transition: margin-top .5s ease-in-out;
    -webkit-transition: margin-top .5s ease-in-out;
  }
  
  #navCheckbox {
    display: none;
  }
  
  #navCheckbox:checked + table {
    margin-top: 0%;
  }
  
  .nav-over, .nav-under {
    display: block;
    max-height: none;
    padding-top: 3px;
    padding-bottom: 3px;
    cursor: pointer;
  }
  
  .nav-over img, .nav-under img {
    display: block;
    float: left;
  }
  
  .nav-over {
    -ms-transition-delay: 1.5s;
    -webkit-transition-delay: 1.5s;
  }
  
  .navContainer > .nav-over:hover + div table {
    margin-top: 0%;
  }
  
  .navContainer > .nav-over:hover {
    visibility: hidden;
  }
  
  body[data-outlook-cycle] #menuContainer, body[data-outlook-cycle] .navContainer {
    display: block;
    position: static;
  }
  
  body[data-outlook-cycle] .navContainer td {
    display: inline-block;
    width: 33.33%;
  }
  
  body[data-outlook-cycle] .navContainer table {
    margin-top: 0;
  }
  
  body[data-outlook-cycle] .navContainer td.appHide {
    display: none;
  }
  
  body[data-outlook-cycle] label {
    display: none;
    }
  }
  
style(type="text/css").

  @media screen and (max-width: 640px) {
    
  u ~ div .wrapper {
