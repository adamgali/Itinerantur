  }
  
  u + #body a {
    color: inherit;
    text-decoration: none;
    font-size: inherit;
    font-family: inherit;
    font-weight: inherit;
    line-height: inherit;
  }
  
  u ~ div .appHide {
    display: none;
  }
  
  u ~ div .appShow {
    display: block;
  }
  
  }
